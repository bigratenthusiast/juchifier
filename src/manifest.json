{
    "manifest_version": 2,

    "name": "Juchifier",
    "description": "Replaces bourgeoisie reactionary calender with the correct one in line with the principles of Juche",
    "version": "1.0.0",
    "browser_action": {
        "default_icon": "icon16.png"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png",
        "512": "icon512.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "index.js"
            ],
            "run_at": "document_end"
        }
    ]
}
